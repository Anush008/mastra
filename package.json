{
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "dts-cli": "^2.0.5",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/config-lerna-scopes": "^19.0.0",
    "commitlint": "^19.4.0",
    "husky": "^9.1.5",
    "lerna": "^8.1.8",
    "lerna-changelog": "^2.2.0",
    "rimraf": "^6.0.1",
    "scripty": "^2.1.1"
  },
  "scripts": {
    "analyze": "scripty",
    "build": "scripty",
    "dev": "scripty",
    "clean": "scripty",
    "lint": "scripty",
    "test": "scripty",
    "lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "size": "scripty",
    "start": "scripty",
    "generate": "scripty",
    "synchronize": "scripty",
    "prettier:format": "prettier --write .",
    "prettier:format:check": "prettier --check .",
    "typecheck": "pnpm -r typecheck",
    "dev:services:up": "docker compose -f .dev/docker-compose.yaml up -d",
    "dev:services:down": "docker compose -f .dev/docker-compose.yaml down"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,json,graphql,gql}": "prettier --write"
  },
  "engines": {
    "pnpm": ">=9.7.0"
  },
  "scripty": {
    "path": "./scripts/workspace"
  },
  "packageManager": "pnpm@9.7.0"
}
